{
  "name": "XOChatModule",
  "version": "0.0.1",
  "summary": "XXOOGO项目的聊天模块",
  "description": "XXOOGO项目采用cocoapods做组件化架构，将不同的模块使用pod私有仓库管理，只需要在主项目中使用 pod 'XOChatModule' 即可导入模块使用",
  "homepage": "http://192.168.1.119/xxoogo_livechat/xochatmodule",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "kenter": "Hjt_830@163.com"
  },
  "source": {
    "git": "http://192.168.1.119/xxoogo_livechat/xochatmodule.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "XOChatModule/Classes/**/*",
  "public_header_files": "XOChatModule/Classes/**/*.h",
  "resource_bundles": {
    "XOChatModule": [
      "XOChatModule/Assets/*"
    ]
  },
  "vendored_libraries": "${PODS_ROOT}/../../XOChatModule/Classes/Lib/lame",
  "frameworks": [
    "UIKit",
    "Foundation"
  ],
  "dependencies": {
    "TXIMSDK_iOS": [
      "~> 4.4.627"
    ],
    "XOBaseLib": [
      "~> 0.1.1"
    ],
    "TZImagePickerController": [
      "~> 3.2.1"
    ],
    "CTMediator": [
      "~> 25"
    ],
    "ReactiveObjC": [
      "~> 3.1.1"
    ],
    "GCDMulticastDelegate": [
      "~> 1.0.0"
    ],
    "SVProgressHUD": [
      "~> 2.2.5"
    ],
    "MBProgressHUD": [
      "~> 1.1.0"
    ]
  }
}
