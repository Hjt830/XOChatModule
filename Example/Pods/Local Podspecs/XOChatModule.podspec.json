{
  "name": "XOChatModule",
  "version": "1.0",
  "summary": "即时通讯项目-聊天模块",
  "description": "XXOOGO项目采用cocoapods做组件化架构，将不同的模块使用pod私有仓库管理，只需要在主项目中使用 pod 'XOChatModule' 即可导入模块使用",
  "homepage": "https://github.com/Hjt830/XOChatModule.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Hjt830": "Hjt_830@163.com"
  },
  "source": {
    "git": "https://github.com/Hjt830/XOChatModule.git",
    "tag": "1.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "XOChatModule/Classes/**/*.{h,m,swift,xcdatamodeld,a,framework}",
  "public_header_files": "XOChatModule/Classes/**/*.h",
  "resource_bundles": {
    "XOChatModule": [
      "XOChatModule/Assets/*"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS[sdk=iphonesimulator*]": ""
  },
  "vendored_libraries": "XOChatModule/Classes/Lib/lame/libmp3lame.a",
  "frameworks": [
    "UIKit",
    "Foundation",
    "CoreLocation"
  ],
  "dependencies": {
    "TXIMSDK_iOS": [
      "~> 4.6.1"
    ],
    "XOBaseLib": [
      "~> 1.0"
    ],
    "TZImagePickerController": [
      "~> 3.2.1"
    ],
    "GCDMulticastDelegate": [
      "~> 1.0.0"
    ],
    "SVProgressHUD": [
      "~> 2.2.5"
    ],
    "AFNetworking": [
      "~> 3.2.1"
    ],
    "YBImageBrowser/NOSD": [
      "~> 3.0.6"
    ],
    "YBImageBrowser/VideoNOSD": [
      "~> 3.0.6"
    ],
    "FLAnimatedImage": [
      "~> 1.0.12"
    ],
    "FMDB": [
      "~> 2.7.5"
    ],
    "YYModel": [
      "~> 1.0.4"
    ],
    "FSTextView": [
      "~> 1.8"
    ]
  }
}
